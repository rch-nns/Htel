{% extends 'base.html.twig' %}

{% block title %}Contact{% endblock %}

{% block body %}
<div class="shine">
<img src="img/contact1.jpg" class="img-fluid" alt="">
</div>
<div class="container">
<h2 class= "mt-5">Contactez-nous</h2>
<hr>
{% for contact in results %}

<div class=" col sm-4 mt-5">
<h4> Adresse : </h4><br>  <p>{{contact.adresse}}</p>
<div class="mt-3">
<iframe src="https://www.google.com/maps/d/u/0/embed?mid=1thEAgNOERQZTBWp6A1pNO3oeW7sJo4bH" width="540" height="380"></iframe>
</div>
<h4 class= "mt-5">Tel : </h4> <p><span>(+33) 0</span>{{ contact.tel | capitalize}}</p>
<h4 class= "mt-5">E-Mail : </h4><br><p>{{ contact.mail | capitalize}}</p>
</div>
</div>
{% endfor %}




  </body>
</html>




{% endblock %}
